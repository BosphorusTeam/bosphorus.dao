    <id name="AccNo" column="AccNo" type="string">
      <generator class="assigned" />
    </id>
    <property name="BaseNo" column="BaseNo" type="string" length="9" />
    <property name="AccClass" column="AccClass" type="string" length="6" />
    <property name="AccDesc" column="AccDesc" type="string" length="35" />
    <property name="OpenBalance" column="OpenBalance" type="Decimal" />
    <property name="CurrBalance" column="CurrBalance" type="Decimal" />
    <property name="Dod" column="DOD" type="Decimal" />
    <property name="Tod" column="TOD" type="Decimal" />
    <property name="ConfTradeTotal" column="ConfTradeTotal" type="Decimal" />
    <property name="Department" column="Department" type="string" length="10" />
    <property name="BondTotal" column="BondTotal" type="Decimal" />
    <property name="SecurityTotal" column="SecurityTotal" type="Decimal" />
  </class>
</hibernate-mapping>